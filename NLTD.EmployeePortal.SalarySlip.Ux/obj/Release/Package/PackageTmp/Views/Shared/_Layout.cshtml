<body class="hold-transition skin-blue sidebar-mini">
    <div id="divLoading" style="margin: 0px; padding: 0px; position: fixed; right: 0px;
    top: 0px; width: 100%; height: 100%; background-color: #666666; z-index: 30001;
    opacity: .8; filter: alpha(opacity=70);display:none">
        <p style="position: absolute; top: 30%; left: 45%; color: White;">
            Loading, please wait...<img src="~/images/ajax-loading.gif">
        </p>
    </div>
    <div class="wrapper">       
        <header class="main-header">
            <a href="/PaySlip" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><img src="~/images/NLTD.png" alt="Northern Lights Technology Development" style="width:30px; padding-top:10px; padding-bottom:10px;" /></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>NLTD</b> SalarySlip</span>
            </a>
            @Html.Partial("~/Views/Shared/AdminLTE/HeaderPartial.cshtml")
        </header>
        @Html.Partial("~/Views/Shared/AdminLTE/SideMenuPartial.cshtml")

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @Html.Partial("~/Views/Shared/AdminLTE/PageHeaderPartial.cshtml")
            <!-- Main content -->
            <section class="content" style="font-size:12px !important">
                <div id="alert_placeholderCommon"> </div>
                @RenderBody()
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->

        @Html.Partial("~/Views/Shared/AdminLTE/FooterPartial.cshtml")
        @Html.Partial("~/Views/Shared/AdminLTE/ControlSideBarLeftPartial.cshtml")
