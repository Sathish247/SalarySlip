    @*<script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>*@

    @*<link rel="stylesheet" type="text/css" href="//cdn.datatables.net/colreorder/1.1.1/css/dataTables.colReorder.css">*@
